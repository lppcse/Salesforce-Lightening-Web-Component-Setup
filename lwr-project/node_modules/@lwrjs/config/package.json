    "scripts": {
        "build": "tsc -b",
        "clean": "rm -rf build node_modules",
        "test": "jest"
    },
    "dependencies": {
        "@lwrjs/diagnostics": "0.10.6",
        "@lwrjs/instrumentation": "0.10.6",
