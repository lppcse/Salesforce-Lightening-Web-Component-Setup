{
    "name": "@lwrjs/config",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "version": "0.10.6",
    "homepage": "https://developer.salesforce.com/docs/platform/lwr/overview",
    "repository": {
        "type": "git",
