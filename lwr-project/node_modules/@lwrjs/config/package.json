        "jsonc-parser": "^3.0.0"
    },
    "devDependencies": {
        "@lwrjs/types": "0.10.6",
        "jest": "^26.6.3",
        "ts-jest": "^26.5.6"
    },
    "peerDependencies": {
        "lwc": "2.x || 3.x"
    },
    "engines": {
        "node": ">=16.0.0"
